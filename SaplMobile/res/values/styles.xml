<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- the theme applied to the application or activity -->
    <style name="CMJCustomActionBarTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
    </style>

    <!-- ActionBar styles -->
    <style name="MyActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">#005347</item>
    </style>

    <style name="ItemListaMaterias">
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">0dp</item>
    </style>

    <style name="TextoExpediente"> 
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="SPMenuTop">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#002200</item>
        <item name="android:height">32dp</item>
    </style>

    <style name="SPMenuSpinner">
        <item name="android:textSize">11sp</item>
        <item name="android:padding">2dp</item>
        <item name="android:dropDownWidth">250dp</item>
    </style>

    <style name="SPMenuSpinnerItem">
        <item name="android:textSize">10sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="SPsubTitleText">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#9a9f9a</item>
        <item name="android:background">#fff</item>
    </style>

    <style name="SPItemListaMateriasFlag">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:width">38dp</item>
        <item name="android:height">24dp</item>
    </style>

    <style name="SPTituloPartes">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#664</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item> 
    </style>

    <style name="SPItemListaMateriasBtnOpenFileImage">
        <item name="android:width">38dp</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#777770</item>
    </style>

    <style name="SPItemListaMateriasEmenta">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#797</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="SPItemListaMateriasProtocolo">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#a00</item>
        <item name="android:paddingLeft">6dp</item>
    </style>

    <style name="SPItemListaMateriasTitulo">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#3b5b90</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">2dp</item>
    </style>

    <style name="SPItemListaMateriasAutor">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#666644</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">1dp</item>
